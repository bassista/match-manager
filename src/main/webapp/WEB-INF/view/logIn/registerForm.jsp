<!--body div-->
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <form action="giocatoreInsert" class="form-horizontal" method="post">
            <fieldset>
                <legend>Register Form</legend>
                <div class="row">
                    <div class="form-group">
                        <span class="col-lg-1 col-md-1 col-sm-1 col-xs-1">First Name:</span>
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                            <input name="firstNameIn" type="text" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <span class="col-lg-1 col-md-1 col-sm-1 col-xs-1">Last Name:</span>
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                            <input name="lastNameIn" type="text" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <span class="col-lg-1 col-md-1 col-sm-1 col-xs-1">Fiscal Code:</span>
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                            <input name="fiscalCodeIn" type="text" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <span class="col-lg-1 col-md-1 col-sm-1 col-xs-1">Mail:</span>
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                            <input name="mailIn" type="email" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <span class="col-lg-1 col-md-1 col-sm-1 col-xs-1">User Name:</span>
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                            <input name="userNameIn" placeholder="admin" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <span class="col-lg-1 col-md-1 col-sm-1 col-xs-1">Password:</span>
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                            <input name="passwordIn" type="password" placeholder="admin" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <input type="submit" class="btn btn-default" value="submit" />
                        </div>
                    </div>
                </div>
            </fieldset>
        </form>
    </div>
</div>